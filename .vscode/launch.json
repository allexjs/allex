{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Attach by Process ID",
      "processId": "${command:PickProcess}",
      "request": "attach",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "Launch Lan Manager",
      "windows": {
        "program": "${env:APPDATA}/npm/node_modules/allex/lanmanager.js",
      },
      "linux": {
        "program": "${env:HOME}/lib/node_modules/allex/lanmanager.js",
      },
      "request": "launch",
      "cwd": "C:/Work/indatacloud",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    },
    {
      "name": "Launch Master",
      "windows": {
        "program": "${env:APPDATA}/npm/node_modules/allex/master.js",
      },
      "linux": {
        "program": "${env:HOME}/lib/node_modules/allex/master.js",
      },
      "request": "launch",
      "cwd": "C:/Work/indatacloud",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "type": "pwa-node"
    }
  ]
}